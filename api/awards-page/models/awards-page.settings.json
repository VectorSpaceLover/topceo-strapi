{
  "connection": "default",
  "collectionName": "awards_pages",
  "info": {
    "name": "Awards Page"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "SubTitle": {
      "type": "string"
    },
    "Image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "ButtonText": {
      "type": "string"
    },
    "ButtonLink": {
      "type": "string"
    },
    "Content": {
      "type": "richtext"
    },
    "SelectSection": {
      "type": "enumeration",
      "enum": [
        "Section1",
        "Section2",
        "Section3",
        "Section4",
        "Section5",
        "Section6",
        "Section7",
        "Section8"
      ]
    }
  }
}
